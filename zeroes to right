public static void main(String[] args) {
	int [] arr = {1,9,8,4,0,0,0,2,7,6,0};
	int n = arr.length;
	int count =0;
	
	int l=0;
	for (int h = 0; h < arr.length; h++) {
			if(arr[h]!=0)
			arr[count++]=arr[h];}
	while(count<n) {
		arr[count++]=0;
	}
	
	System.out.print("\n");
	for (int i = 0; i < arr.length; i++) {
		System.out.print(arr[i]+" ");
	}
	
}
}
